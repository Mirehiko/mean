{
  "name": "mean",
  "version": "1.0.0",
  "description": "Data by MEAN (Mongo, Express, Angular, Node.js) course",
  "main": "index.js",
  "engines": {
    "node": "v12.18.2",
    "npm": "6.14.8"
  },
  "scripts": {
    "start": "node index.js",
    "server": "nodemon index.js",
    "client-install": "npm i --prefix client",
    "client": "npm run start --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client\" ",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm run client-install && npm run build --prefix client"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Mirehiko/mean.git"
  },
  "keywords": [
    "MongoDB",
    "Express",
    "Node.js",
    "Angular"
  ],
  "author": "mirehiko",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Mirehiko/mean/issues"
  },
  "homepage": "https://github.com/Mirehiko/mean#readme",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "moment": "^2.27.0",
    "mongoose": "^5.10.0",
    "morgan": "^1.10.0",
    "multer": "^1.4.2",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0"
  },
  "devDependencies": {
    "concurrently": "^5.3.0",
    "nodemon": "^2.0.4"
  }
}
